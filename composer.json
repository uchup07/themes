{
    "name": "uchup07/themes",
    "description": "Laravel Themes",
    "keywords": ["themes", "laravel", "caffeinated", "blade"],
    "license": "MIT",
    "authors": [
        {
            "name": "Shea Lewis",
            "email": "shea.lewis89@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.3|^8.0|^8.1",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "files": [
            "helpers/themes.php"
        ],
        "psr-4": {
            "Uchup07\\Themes\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Uchup07\\Themes\\ThemesServiceProvider"
            ],
            "aliases": {
                "Theme": "Uchup07\\Themes\\Facades\\Theme"
            }
        }
    }
}
